# 1
# 1
add wave -position insertpoint  \
sim:/computador/ADDR \
sim:/computador/DOUT \
sim:/computador/DIN \
sim:/computador/R0_out \
sim:/computador/R1_out \
sim:/computador/R2_out \
sim:/computador/R3_out \
sim:/computador/R4_out \
sim:/computador/R5_out \
sim:/computador/R7_out
add wave -position insertpoint  \
sim:/computador/done
add wave -position insertpoint  \
sim:/computador/W
add wave -position insertpoint  \
sim:/computador/processador/incr_pc
add wave -position insertpoint  \
sim:/computador/processador/regs_in
add wave -position insertpoint  \
sim:/computador/processador/mux_selector
add wave -position insertpoint  \
sim:/computador/processador/ula_op
wave modify -driver freeze -pattern counter -startvalue 000 -endvalue 001 -type Range -direction Up -period 2ps -step 1 -repeat forever -range 2 0 -starttime 0ps -endtime 1500ps Edit:/computador/KEY
run -all
add wave -position insertpoint  \
sim:/computador/processador/cont
run -all
restart
